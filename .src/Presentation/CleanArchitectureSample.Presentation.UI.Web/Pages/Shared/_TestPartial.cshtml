@model CleanArchitectureSample.Presentation.UI.Web.Areas.Packages.Pages.TestModel.NestedViewModel
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<form method="post">
    <div>
        <label asp-for="Address"></label>
        <input type="text" name="txtAddress" asp-for="Address" value="" />

    </div>
    <div>
        <label asp-for="Contact"></label>
        <input type="text" name="txtContact" asp-for="Contact" value="" />
    </div>
    <button type="submit" >submit</button>

</form>
<div>
    @if (Model?.NestedList != null)
    {
        <table>
            <thead>
                <tr>
                    <th><label asp-for="Address"></label></th>
                    <th><label asp-for="Contact"></label></th>
                </tr>
            </thead>
            @foreach (var item in Model.NestedList)
            {
                <tr>
                    <td>@item.Address</td>
                    <td>@item.Contact</td>
                </tr>
            }
        </table>
    }

</div>